{
  
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],

  "cleanUrls": true,

  "trailingSlash": false,

  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
